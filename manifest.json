{
  "name": "Don't show my face on SOL!",
  "version": "1.1",
  "manifest_version": 3,
  "description": "SOLのサイドバーに表示されるプロフィール写真を非表示にします。",
  "content_scripts": [
    {
      "matches": ["*://sol.sfc.keio.ac.jp/*"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}
